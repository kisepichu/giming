{
  "version": "0.2.0",
  "configurations": [
    {% for wp in work_problems %}
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug {{ wp.problem.code }}",
      "cargo": {
        "args": ["build", "--bin={{ wp.problem.id }}"],
        "filter": {
          "name": "{{ wp.problem.id }}",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {% endfor %}
  ]
}
